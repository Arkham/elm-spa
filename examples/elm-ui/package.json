{
  "name": "elm-spa-elm-ui",
  "version": "1.0.0",
  "description": "an elm-ui example of ryannhg/elm-spa",
  "scripts": {
    "start": "npm install && npm run dev",
    "build": "elm make src/Main.elm --output=public/dist/elm.compiled.js --optimize",
    "dev": "elm-live src/Main.elm --dir=public -u -- --output=public/dist/elm.compiled.js --debug"
  },
  "author": "Ryan Haskell-Glatz",
  "license": "ISC",
  "devDependencies": {
    "elm": "0.19.1-3",
    "elm-live": "4.0.1"
  }
}
